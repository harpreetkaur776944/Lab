<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:verticalScrollbarPosition="right"
    tools:context=".FeedbackFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="658dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="302dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="594dp"
        android:fontFamily="@font/basic"
        android:text="Select Home Collection Date"
        android:textColor="@color/myColor"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageViewCalendar"
        android:layout_width="38dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="350dp"
        android:layout_marginTop="93dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="600dp"
        android:src="@drawable/icon_calendar"
        android:tint="@color/myColor" />

    <EditText
        android:id="@+id/editTextFeedName"
        android:layout_width="350dp"
        android:layout_height="62dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="524dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editTextContact"
        android:layout_width="349dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="453dp"
        android:ems="10"
        android:hint="Mobile Number"
        android:inputType="number" />

    <TextView
        android:id="@+id/textViewQues1"
        android:layout_width="351dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="304dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="366dp"
        android:fontFamily="@font/basic"
        android:text="1. Did the Phlebotomist reach on scheduled time?"
        android:textSize="18sp" />

    <RadioGroup
        android:id="@+id/radioGroupQues1"
        android:layout_width="wrap_content"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="375dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="302dp"
        android:orientation="horizontal">

        <RadioButton
            android:id="@+id/radioButton7"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:text="Yes" />

        <RadioButton
            android:id="@+id/radioButton8"
            android:layout_width="173dp"
            android:layout_height="wrap_content"
            android:text="No" />

    </RadioGroup>

    <TextView
        android:id="@+id/textViewQue2"
        android:layout_width="358dp"
        android:layout_height="71dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="434dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="226dp"
        android:layout_weight="1"
        android:fontFamily="@font/basic"
        android:text="2. Was the phlebotomist efficient and able to answer all the queries?"
        android:textSize="18sp" />

    <RadioGroup
        android:id="@+id/radioGroupQues2"
        android:layout_width="wrap_content"
        android:layout_height="53dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="510dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="168dp"
        android:orientation="horizontal">

        <RadioButton
            android:id="@+id/radioButton9"
            android:layout_width="161dp"
            android:layout_height="wrap_content"
            android:text="Yes" />

        <RadioButton
            android:id="@+id/radioButton10"
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:text="No" />
    </RadioGroup>

    <Button
        android:id="@+id/buttonSubmit2"
        android:layout_width="156dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="128dp"
        android:layout_marginTop="575dp"
        android:layout_marginEnd="126dp"
        android:layout_marginBottom="108dp"
        android:background="@drawable/round_button"
        android:backgroundTint="@color/myColor"
        android:text="Submit"
        android:textColor="@android:color/white"
        android:textStyle="bold" />

</RelativeLayout>